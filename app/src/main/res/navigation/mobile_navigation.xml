<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.instagraminterface.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.instagraminterface.ui.search.SearchFragment"
        android:label="search"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_video"
        android:name="com.example.instagraminterface.ui.video.VideoFragment"
        android:label="Video"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_like"
        android:name="com.example.instagraminterface.ui.like.LikeFragment"
        android:label="Likes"
        tools:layout="@layout/fragment_like" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.instagraminterface.ui.user.UserFragment"
        android:label="User"
        tools:layout="@layout/fragment_user" />

</navigation>